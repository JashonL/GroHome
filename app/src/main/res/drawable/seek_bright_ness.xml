<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:bottom="5dp"
        android:top="5dp">
        <shape>
            <corners android:radius="@dimen/dp_12" />
            <solid android:color="@color/color_white_mode_panel_title" />
            <size android:width="@dimen/dp_240" android:height="@dimen/dp_50"/>
        </shape>
    </item>

    <item
        android:id="@android:id/progress"
        android:bottom="5dp"
        android:top="5dp">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_12" />
                <solid android:color="@color/seek_progress" />
            </shape>
        </clip>
    </item>
</layer-list>