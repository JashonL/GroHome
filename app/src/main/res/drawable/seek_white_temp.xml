<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:bottom="5dp"
        android:top="5dp">
        <shape>
            <corners android:radius="@dimen/dp_12" />
            <gradient
                android:angle="0"
                android:endColor="@color/seek_gradient_start"
                android:startColor="@color/white" />
        </shape>
    </item>

    <item
        android:id="@android:id/background"
        android:bottom="5dp"
        android:top="5dp">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_12" />
                <gradient
                    android:angle="0"
                    android:endColor="@color/seek_gradient_start"
                    android:startColor="@color/white" />
            </shape>
        </clip>
    </item>

</layer-list>